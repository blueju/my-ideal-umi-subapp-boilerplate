{
  "name": "my-ideal-umi-subapp-boilerplate",
  "description": "我理想中的 umi 子应用脚手架",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build:dev": "cross-env UMI_ENV=dev umi build",
    "build:dvl": "cross-env UMI_ENV=dvl umi build",
    "build:sit": "cross-env UMI_ENV=sit umi build",
    "build:uat": "cross-env UMI_ENV=uat umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/preset-react": "1.x",
    "lodash": "^4.17.21",
    "umi": "^3.4.14"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-qiankun": "^2.24.1",
    "@umijs/test": "^3.4.14",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-alloy": "^4.1.0",
    "eslint-plugin-react": "^7.23.2",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
